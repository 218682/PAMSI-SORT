/*!
\file
\brief plik zawiera funkcje z metodami sortowania
*/
#ifndef SORT_HH
#define SORT_HH
void bubblesort(int tab[], int size)
{
  int tmp;
  for(int i=0; i< size; i++)
    {
      for(int j=0; j<size; j++)
	{
	  if(tab[j]>tab[j+1])
	    {
	      tmp=tab[j];
	      tab[j]=tab[j+1];
	      tab[j+1]=tmp;
	    }
	}
    }
}
#endif
